{
	"name": "APIdog Plus",
	"short_name": "APIdog+",
	"description": "Расширение, позволяющее расширить возможности работы сайта APIdog.",
	"version": "3.4.0",
	"manifest_version": 3,
	"author": "APIdog",
	"homepage_url": "https://apidog.ru/plus",
	"icons": {
		"114": "images/114.png",
		"64": "images/64.png"
	},
	"default_locale": "ru",
	"permissions": [
		"tabs",
		"declarativeNetRequest",
		"contextMenus"
	],
	"host_permissions": [
		"*://apidog.ru/*",
		"*://*.apidog.ru/*",
		"*://*.vk.com/*",
		"*://*.vk.ru/*",
		"*://*.vk.me/*",
		"*://*.mycdn.me/*"
	],
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["*://apidog.ru/*", "*://*.apidog.ru/*"],
			"js": ["config.js", "libapidog0.js"],
			"all_frames": true
		}
	],
	"declarative_net_request": {
		"rule_resources": [
			{
				"id": "main_ruleset",
				"enabled": true,
				"path": "rules.json"
			  }
		]
	}
}
