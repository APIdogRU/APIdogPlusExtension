{
	"name": "APIdog Plus",
	"short_name": "APIdog+",
	"description": "Расширение, позволяющее расширить возможности работы сайта APIdog.",
	"version": "2.0",
	"manifest_version": 2,
	"author": "APIdog",
	"homepage_url": "http://apidog.ru/extensions",
	"icons": {
		"114": "icon114.png",
		"64": "icon64.png"
	},
	"permissions": [
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"*://apidog.ru/*",
		"*://*.apidog.ru/*",
		"*://*.vk.com/*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"matches": ["*://apidog.ru/*"],
		"js": ["APIdogLib.js", "longpoll.js"],
		"all_frames": true
	}]
}