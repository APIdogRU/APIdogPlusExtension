{
	"name": "APIdog Plus",
	"short_name": "APIdog+",
	"description": "Расширение, позволяющее расширить возможности работы сайта APIdog.",
	"version": "2.1",
	"manifest_version": 2,
	"author": "APIdog",
	"homepage_url": "http://apidog.ru/extensions",
	"icons": {
		"114": "images/114.png",
		"64": "images/64.png"
	},
	"permissions": [
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["API.js", "APIdogLib.js", "longpoll.js"],
		"all_frames": true
	}]
}
